{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "tcomb",
    "transform-decorators-legacy",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack/test.config.js", "verbose": false }]
      ]
    },
    "production": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-runtime"
      ]
    }
  }
}
